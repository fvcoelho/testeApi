<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    @RenderSection("PageScripts",false)
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>MVC State/City</h1>
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("List States", "Index", "State")</li>
                    <li>@Html.ActionLink("New States", "Create", "State")</li>
                    <li>@Html.ActionLink("List Cities", "Index", "City")</li>
                    <li>@Html.ActionLink("New Cities", "Create", "City")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
